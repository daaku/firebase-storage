  },
  "keywords": ["firebase", "api"],
  "devDependencies": {
    "@daaku/firebase-auth": "^2.2.0",
    "@daaku/qunit-puppeteer": "^3.0.0",
    "@types/qunit": "^2.11.2",
    "esbuild": "^0.20.0",
    "nanoid": "^5.0.1",
    "qunit": "^2.17.2",
    "typescript": "^5.0.2"
  }
}
